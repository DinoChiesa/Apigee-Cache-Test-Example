<PopulateCache name='PopulateCache-With-Prefix'>

<!--
  <Scope>Application</Scope>
-->
  <Source>request.content</Source>

  <ExpirySettings>
    <TimeoutInSeconds>180</TimeoutInSeconds>
  </ExpirySettings>

  <CacheKey>
    <Prefix>myprefix</Prefix>
    <KeyFragment ref='request.queryparam.id' />
  </CacheKey>

</PopulateCache>
